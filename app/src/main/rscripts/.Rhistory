calcMeanNrgs <- function(tables)
{
nrg.list <- c()
for (i in 1:length(tables))
{
table = as.matrix(tablelist[[i]])
#pept = table[,1]
#cdr3 = table[1,]
nrg.list = append(nrg.list, sum(apply(table[-1, -1], c(1, 2), as.numeric), c(1, 2)))
}
return(nrg.list)
}
